<!--<h2>User info</h2>-->
<div class="container">
  <div class="row">
    <div class="col-md-3">
<%= render 'users/list', user: @user %>

<!--<h2>New book</h2>-->
<%= render 'books/list', book: @book %>
   </div>

<div class="col-md-8 offset-md-1">
  <table class="table">
  <h2>Users</h2>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>






<% @users.each do |user| %>

  <thead>
    <tr>
      <th><%= attachment_image_tag user, :profile_image, :fill, 50, 50, fallback: "no_image.jpg", size:'50x50' %></th>
      <th><%= user.name %></th>
      <th><%= link_to 'Show', user_path(user.id) %></th>
    </tr>
  </thead>
<% end %>
</table>
</div>
</div>
</div>
</div>
